{
	"name": "ShorterLinker",
	"version": "1.0.0",
	"description": "Shorter linker caster",
	"keywords": [
		"express",
		"typescript",
		"LongLinker cast to shorter link"
	],
	"license": "MUT",
	"main": "lib/app.js",
	"engines": {
		"node": ">=8.2"
	},
	"scripts": {
		"build": "npm run lint && npm run clear && tsc && abspath",
		"lint": "tslint --project tsconfig.json --config tslint.json --format stylish",
		"prod": "cross-env NODE_ENV=production node .",
		"serve": "cross-env NODE_ENV=development NODE_PATH=src nodemon -e ts -w src -x ts-node --inspect src/app.ts"
	},
	"dependencies": {
		"@types/redis": "^2.8.17",
		"body-parser": "^1.18.2",
		"cross-env": "^5.1.1",
		"express": "^4.16.2",
		"morgan": "^1.9.0",
		"redis": "^3.0.2",
		"winston": "^2.4.0"
	},
	"devDependencies": {
		"@types/body-parser": "^1.16.8",
		"@types/chai": "^4.0.0",
		"@types/express": "^4.0.39",
		"@types/mocha": "^2.2.44",
		"@types/morgan": "^1.7.35",
		"@types/node": "^8.0.0",
		"@types/winston": "^2.3.7",
		"absolute-path-converter": "^1.0.4",
		"chai": "^4.0.0",
		"del-cli": "^1.1.0",
		"mocha": "^4.0.0",
		"nodemon": "^1.12.1",
		"ts-node": "^3.0.0",
		"tslint": "^5.8.0",
		"tslint-config-airbnb": "^5.4.0",
		"typescript": "^2.6.1"
	}
}
